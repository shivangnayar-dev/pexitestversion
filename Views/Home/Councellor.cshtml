            background: url("https://media.licdn.com/dms/image/C5603AQHKvoMsPrvwIw/profile-displayphoto-shrink_400_400/0/1613379952915?e=1715212800&v=beta&t=VHarV89foqytDtSQvcIQLNUJrv0bmgoPRQSRkgPsF7I") no-repeat;
