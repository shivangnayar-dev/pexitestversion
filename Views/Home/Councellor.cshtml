                font-size: 0.8em;
                opacity: 0.5;
            }

        .my-message {
            justify-content: flex-start;
        }

        .friend-message p {
            background: #fff;
        }

        .friend-message {
            justify-content: flex-start;
        }

        .chat-container .my-message i {
            color: yellow;
        }

        .message-box.friend-message::before {
            content: '';
            position: absolute;
            top: 0;
            left: -12px;
            width: 20px;
            height: 20px;
            background: linear-gradient(225deg, #fff 0%, #fff 50%, transparent 50%, transparent);
        }

        .chatbox-input {
            position: relative;
            width: 100%;
            background: #f0f0f0;
            display: flex;
            justify-content: space-between;
            align-items: center;
        }

