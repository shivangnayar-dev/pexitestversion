        .front,
        .back {
            position: relative;
            top: 0;
            left: 0;
            backface-visibility: hidden;
        }

        .front {
            cursor: pointer;
            height: 100%;
            background: white;
            backface-visibility: hidden;
            border-radius: 5px;
            box-shadow: 0 15px 10px -10px rgba(0, 0, 0, 0.5), 0 1px 4px rgba(0, 0, 0, 0.3), 0 0 40px rgba(0, 0, 0, 0.1) inset;
        }
        .outer-div {
