            height: $card-height;
            max-width: $card-width;
            margin: 0 auto;
            position: relative;
        }

      

        .inner-div {
            margin: 0 auto;
            border-radius: 5px;
            font-weight: 400;
            color: $black;
            font-size: 1rem;
            text-align: center;
            transition: all 0.6s cubic-bezier(0.8, -0.4, 0.2, 1.7);
